{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Subnet template",
    "Resources": {
        "ChildStackVPC": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "TemplateURL": "https://s3.eu-central-1.amazonaws.com/lavinia-dev-nested/VPC.json",
                "TimeoutInMinutes": "50"
            }
        },
        "newSubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Fn::GetAtt": [
                        "ChildStackVPC",
                        "Outputs.VPC"
                    ]
                },
                "CidrBlock": "10.0.0.0/24",
                "AvailabilityZone": "eu-central-1a",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Subnet"
                    }
                ]
            }
        }
    },
    "Outputs": {
        "Subnet": {
            "Description": "Information about the Subnet",
            "Value": {
                "Ref": "newSubnet"
            }
        },
        "VPC": {
            "Description": "Information about the Subnet",
            "Value": {
                "Fn::GetAtt": [
                    "ChildStackVPC",
                    "Outputs.VPC"
                ]
            }
        }
    }
}